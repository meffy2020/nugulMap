# Public API endpoint used by the app
EXPO_PUBLIC_API_BASE_URL=https://api.nugulmap.com

# Kakao map/js key
EXPO_PUBLIC_KAKAO_JAVASCRIPT_KEY=YOUR_KAKAO_JS_KEY

# Must be a public https domain registered in Kakao console
EXPO_PUBLIC_KAKAO_WEBVIEW_BASE_URL=https://nugulmap.com
EXPO_PUBLIC_KAKAO_MARKER_IMAGE_URL=https://nugulmap.com/images/pin.png

# OAuth deep-link callback
EXPO_PUBLIC_OAUTH_REDIRECT_URI=nugulmap://oauth/callback

# Increment on every store submission
APP_IOS_BUILD_NUMBER=1
APP_ANDROID_VERSION_CODE=1
